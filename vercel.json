{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/stripe/(.*)",
      "dest": "/server/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server/index.js"
    },
    {
      "src": "/success",
      "dest": "/success.html"
    },
    {
      "src": "/cancel",
      "dest": "/cancel.html"
    },
    {
      "src": "/kundendaten",
      "dest": "/kundendaten.html"
    },
    {
      "src": "/datenschutz",
      "dest": "/datenschutz.html"
    },
    {
      "src": "/impressum",
      "dest": "/impressum.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}